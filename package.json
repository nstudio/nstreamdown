    "@nx/vite": "^22.3.3",
    "@nx/vitest": "^22.3.3",
    "@types/react": "npm:types-react-without-jsx-intrinsics@^18.0.17",
    "husky": "~9.0.0",
    "ng-packagr": "^21.0.1",
    "patch-package": "^8.0.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react": "~18.2.0",
    "react-nativescript": "^5.0.0",
